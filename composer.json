{
  "name": "rootlocal/yii2-multiselect",
  "description": "MultiSelect Widget for Yii2",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "multiselect"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Alexander Zakharov",
      "email": "sys@eml.ru",
      "homepage": "https://github.com/rootlocal/yii2-multiselect"
    }
  ],
  "support": {
    "issues": "https://github.com/rootlocal/yii2-multiselect/issues?state=open",
    "source": "https://github.com/rootlocal/yii2-multiselect"
  },
  "require": {
    "php": ">=7.1",
    "yiisoft/yii2": "~2.0",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "2amigos/yii2-multi-select-widget": "*",
    "bower-asset/quicksearch": "dev-master"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.1.0",
    "yiisoft/yii2-faker": "~2.0.0",
    "codeception/codeception": "^4.0",
    "codeception/module-asserts": "^1.0",
    "codeception/module-yii2": "^1.0",
    "codeception/module-filesystem": "^1.0",
    "codeception/verify": "~0.5.0 || ~1.1.0",
    "symfony/browser-kit": ">=2.7 <=4.2.4"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "autoload": {
    "psr-4": {
      "rootlocal\\widgets\\multiselect\\": "src/"
    }
  },
  "extra": {
    "bootstrap": "rootlocal\\widgets\\multiselect\\Bootstrap"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
